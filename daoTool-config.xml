<?xml version="1.0" encoding="UTF-8"?>
<daoTool xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="./metadata/jaxb/DAOTool.xsd">

	<filePath>metadata/gen</filePath>
	<project>org.conan</project>
	<dbname>bank</dbname>
	<dbtype>mysql</dbtype>
	
	<modules>
		<module>
			<name>bank</name>
			<model>
				<name>bank</name>
				<table>t_bank</table>
				<prop><name>bank_name</name><type>varchar(128)</type><unique>true</unique></prop>
				<prop><name>create_date</name><type>timestamp</type><default>now()</default></prop>
			</model>
			<model>
				<name>bankLog</name>
				<table>t_bank_log</table>
				<prop><name>bankid</name><type>int</type></prop>
				<prop><name>tran_ref_no</name><type>varchar(16)</type></prop>
				<prop><name>code</name><type>varchar(32)</type></prop>
				<prop><name>description</name><type>text</type></prop>
				<prop><name>create_date</name><type>timestamp</type></prop>
			</model>
			<model>
				<name>bankTransfer</name>
				<table>t_bank_transfer</table>
				<prop><name>bankid</name><type>int</type></prop>
				<prop><name>tran_date</name><type>varchar(8)</type><null>true</null></prop>
				<prop><name>tran_time</name><type>varchar(6)</type><null>true</null></prop>
				<prop><name>tran_flag</name><type>varchar(8)</type><null>true</null></prop>
				<prop><name>tran_msg</name><type>varchar(512)</type><null>true</null></prop>
				<prop><name>taocan_flag</name><type>varchar(8)</type><null>true</null></prop>
				<prop><name>taocan_type</name><type>varchar(8)</type><null>true</null></prop>
				<prop><name>tran_fee</name><type>varchar(16)</type><null>true</null></prop>
				<prop><name>p_flowno</name><type>varchar(16)</type><null>true</null></prop>
				<prop><name>tran_ref_no</name><type>varchar(16)</type></prop>
			</model>
		</module>
	</modules>
</daoTool>