<?xml version="1.0" encoding="UTF-8"?>
<daoTool xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="./metadata/jaxb/DAOTool.xsd">

	<filePath>metadata/gen</filePath>
	<project>org.conan.fans</project>
	<dbname>fans</dbname>
	<dbtype>mysql</dbtype>
	
	<modules>
		<module>
			<name>weibo</name>
            <model>
                <name>account</name>
                <table>t_account</table>
                <prop><name>uid</name><type>long</type><unique>true</unique></prop>
                <prop><name>create_date</name><type>timestamp</type><default>now()</default><null>true</null></prop>
                <prop><name>expireIn</name><type>varchar(16)</type><null>true</null></prop>
                <prop><name>refresh</name><type>varchar(32)</type><null>true</null></prop>
                <prop><name>state</name><type>varchar(32)</type><null>true</null></prop>
                <prop><name>screen_name</name><type>varchar(32)</type><unique>true</unique></prop>
                <prop><name>token</name><type>varchar(32)</type></prop>
            </model>
			<model>
				<name>userRelate</name>
				<table>t_user_relate</table>
				<prop><name>uid</name><type>long</type></prop>
				<prop><name>fansid</name><type>long</type></prop>
				<prop><name>create_date</name><type>timestamp</type><default>now()</default><null>true</null></prop>
                <index><type>unique</type><name>uid</name><name>fansid</name></index>
			</model>
            <model>
              <name>userBirelate</name>
              <table>t_user_birelate</table>
              <prop><name>uid</name><type>long</type></prop>
              <prop><name>biuid</name><type>long</type></prop>
              <prop><name>create_date</name><type>timestamp</type><default>now()</default><null>true</null></prop>
              <index><type>unique</type><name>uid</name><name>biuid</name></index>
            </model>
			<model>
				<name>user</name>
				<table>t_user</table>
				<prop><name>uid</name><type>long</type><unique>true</unique></prop>
				<prop><name>screen_name</name><type>varchar(32)</type><unique>true</unique></prop>
				<prop><name>name</name><type>varchar(32)</type><unique>true</unique></prop>
				<prop><name>province</name><type>int</type><null>true</null></prop>
				<prop><name>city</name><type>int</type><null>true</null></prop>
				<prop><name>location</name><type>varchar(32)</type><null>true</null></prop>
				<prop><name>description</name><type>varchar(128)</type><null>true</null></prop>
				<prop><name>url</name><type>varchar(128)</type><null>true</null></prop>
				<prop><name>profile_image_url</name><type>varchar(128)</type><null>true</null></prop>
				<prop><name>domain</name><type>varchar(32)</type><null>true</null></prop>
				<prop><name>gender</name><type>varchar(1)</type><default>m</default><null>true</null></prop>
				<prop><name>followers_count</name><type>int</type><default>0</default></prop>
				<prop><name>friends_count</name><type>int</type><default>0</default></prop>
				<prop><name>statuses_count</name><type>int</type><default>0</default></prop>
				<prop><name>favourites_count</name><type>int</type><default>0</default></prop>
				<prop><name>created_at</name><type>datetime</type></prop>
				<prop><name>allow_all_act_msg</name><type>varchar(1)</type><default>f</default><null>true</null></prop>
				<prop><name>remark</name><type>varchar(32)</type><null>true</null></prop>
				<prop><name>geo_enabled</name><type>varchar(1)</type><default>t</default><null>true</null></prop>
				<prop><name>verified</name><type>varchar(1)</type><default>f</default><null>true</null></prop>
				<prop><name>allow_all_comment</name><type>varchar(1)</type><default>t</default><null>true</null></prop>
				<prop><name>avatar_large</name><type>varchar(128)</type><null>true</null></prop>
				<prop><name>verified_reason</name><type>varchar(32)</type><null>true</null></prop>
				<prop><name>online_status</name><type>int</type><null>true</null></prop>
				<prop><name>lang</name><type>varchar(8)</type><null>true</null></prop>
				<prop><name>weihao</name><type>varchar(32)</type><null>true</null></prop>
				<prop><name>verifiedType</name><type>int</type><null>true</null></prop>
				<prop><name>create_date</name><type>timestamp</type><default>now()</default><null>true</null></prop>
			</model>
            <model>
                <name>userIncrease</name>
                <table>t_user_increase</table>
                <prop><name>uid</name><type>long</type></prop>
                <prop><name>date</name><type>int</type></prop>
                <prop><name>type</name><type>varchar(8)</type></prop>
                <prop><name>count</name><type>long</type><default>0</default></prop>
                <index><type>unique</type><name>uid</name><name>date</name><name>type</name></index>
                <index><name>date</name></index>
            </model>
			<model>
				<name>provinces</name>
				<table>t_provinces</table>
				<prop><name>name</name><type>varchar(16)</type></prop>
			</model>
		</module>
		<module>
			<name>system</name>
            <model>
                <name>config</name>
                <table>t_config</table>
                <prop><name>create_date</name><type>timestamp</type><default>now()</default><null>true</null></prop>
                <prop><name>imgUrl</name><type>varchar(256)</type><null>true</null></prop>
                <prop><name>template</name><type>varchar(256)</type><null>true</null></prop>
                <prop><name>r</name><type>varchar(256)</type><null>true</null></prop>
                <prop><name>output</name><type>varchar(256)</type><null>true</null></prop>
                <prop><name>type</name><type>varchar(16)</type><unique>true</unique></prop>
            </model>
            <model>
                <name>limitUser</name>
                <table>t_limit_user</table>
                <prop><name>uid</name><type>long</type></prop>
                <prop><name>name</name><type>varchar(16)</type></prop>
                <prop><name>limit_time</name><type>long</type><null>true</null><default>-1</default></prop>
                <prop><name>create_date</name><type>timestamp</type><default>now()</default><null>true</null></prop>
                <index><type>unique</type><name>uid</name><name>name</name></index>
            </model>
        </module>
	</modules>
</daoTool>