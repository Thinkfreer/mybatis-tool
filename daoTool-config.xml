<?xml version="1.0" encoding="UTF-8"?>
<daoTool xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="./metadata/jaxb/DAOTool.xsd">

	<filePath>metadata/gen</filePath>
	<project>org.conan</project>
	<dbname>bank</dbname>
	
	<modules>
		<module>
			<name>bank</name>
			<model>
				<name>bank</name>
				<table>t_bank</table>
				<prop><name>bank_name</name><type>String</type></prop>
				<prop><name>create_date</name><type>Timestamp</type></prop>
			</model>
			<model>
				<name>bankLog</name>
				<table>t_bank_log</table>
				<prop><name>bankid</name><type>Integer</type></prop>
				<prop><name>tran_ref_no</name><type>String</type></prop>
				<prop><name>code</name><type>String</type></prop>
				<prop><name>description</name><type>String</type></prop>
				<prop><name>create_date</name><type>Timestamp</type></prop>
			</model>
			<model>
				<name>bankTransfer</name>
				<table>t_bank_transfer</table>
				<prop><name>bankid</name><type>Integer</type></prop>
				<prop><name>tran_date</name><type>String</type><null>true</null></prop>
				<prop><name>tran_time</name><type>String</type><null>true</null></prop>
				<prop><name>tran_flag</name><type>String</type><null>true</null></prop>
				<prop><name>tran_msg</name><type>String</type><null>true</null></prop>
				<prop><name>taocan_flag</name><type>String</type><null>true</null></prop>
				<prop><name>taocan_type</name><type>String</type><null>true</null></prop>
				<prop><name>tran_fee</name><type>String</type><null>true</null></prop>
				<prop><name>p_flowno</name><type>String</type><null>true</null></prop>
				<prop><name>tran_ref_no</name><type>String</type></prop>
			</model>
		</module>
		<module>
			<name>user</name>
			<model>
				<name>user</name>
				<table>t_user</table>
				<prop><name>name</name><type>String</type></prop>
				<prop><name>create_date</name><type>Timestamp</type></prop>
			</model>
		</module>
	</modules>
</daoTool>